@model OnlineTest.Models.Test

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<div>
    <h4>Test</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("Test Name")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Test_Name)
        </dd>

        <dt>
            @Html.DisplayName("Description")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Test_Description)
        </dd>

        <dt>
            @Html.DisplayName("Total Marks")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Test_Marks)
        </dd>

        <dt>
            @Html.DisplayName("Passing Marks")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Test_PassMarks)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.Test_Id }) |
    @Html.ActionLink("Back to List", "Index")
</p>
